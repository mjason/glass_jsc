<% @funcs.each do |func| %>
var glassx.sdk.<%= func[:name] %> = function (<%= func[:options].keys.join(", ") %>) {
  var data = {}
  <% func[:options].delete(:callback) %>
  <% func[:options].each do |key, value| %>
  <% if value == fun %>
  data["<%= value + key.to_s  %>"] = <%= key %>.toString()
  <% else %>
  data["<%= value + key.to_s  %>"] = <%= key %>
  <% end %>
  <% end %>
  glassx.sdk.adb("<%= func[:alias] %>", data, callback)
}
<% end %>
